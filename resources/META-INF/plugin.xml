<idea-plugin version="2" url="https://github.com/hajdam/defaultpopup-intellij-plugin">
    <id>org.exbin.utils.defaultpopup</id>
    <name>Default Popup Menu</name>
    <version>0.1.0</version>
    <vendor email="hajdam@users.sf.net" url="https://exbin.org">ExBin Project</vendor>

    <description><![CDATA[
<p>Registers basic popup menu in IDE itself for regular components like text fields, lists and tables to perform basic clipboard operations like cut/copy/paste/delete and select all.</p>

<h1>Preview</h1>
<p><img src="https://exbin.org/images/defaultpopup-intellij-preview-0.1.0.png" alt="defaultpopup-intellij-preview"/></p>

<h1>License</h1>
<a href="http://www.apache.org/licenses/LICENSE-2.0">Apache License, Version 2.0</a>

<h1>Links</h1>
Sources: <a href="https://github.com/hajdam/defaultpopup-intellij-plugin">https://github.com/hajdam/defaultpopup-intellij-plugin</a>
    ]]></description>

    <!-- please see http://www.jetbrains.org/intellij/sdk/docs/basics/getting_started/build_number_ranges.html for description -->
    <idea-version since-build="145.0"/>

    <!-- please see http://www.jetbrains.org/intellij/sdk/docs/basics/getting_started/plugin_compatibility.html
         on how to target different products -->
    <depends>com.intellij.modules.lang</depends>

    <project-components>
    </project-components>

    <extensions defaultExtensionNs="com.intellij">
        <postStartupActivity implementation="org.exbin.utils.defaultpopup.DefaultPopupInstaller" /> -->
        <!-- <preloadingActivity implementation="org.exbin.utils.defaultpopup.DefaultPopupInstaller" /> -->
    </extensions>
</idea-plugin>
